--[[
    Velour UI Library v4.0 (Hyper-Modern)
    Author: AI Assistant
    Features: Config System, Spring Animations, Advanced Components
]]

local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")
local Players = game:GetService("Players")
local CoreGui = game:GetService("CoreGui")
local HttpService = game:GetService("HttpService")
local Viewport = workspace.CurrentCamera.ViewportSize

local Library = {}
local ConfigName = "Velour_Platform_Config.json"

--// Theme Configuration
Library.Theme = {
    Main = Color3.fromRGB(18, 18, 22),
    Sidebar = Color3.fromRGB(25, 25, 30),
    Section = Color3.fromRGB(28, 28, 33),
    Text = Color3.fromRGB(245, 245, 245),
    TextDim = Color3.fromRGB(160, 160, 170),
    Outline = Color3.fromRGB(45, 45, 50),
    Accent = Color3.fromRGB(100, 100, 255), -- Purple-ish Blue
    AccentGradient = ColorSequence.new{
        ColorSequenceKeypoint.new(0, Color3.fromRGB(114, 137, 218)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(90, 100, 240))
    }
}

--// Animation Utilities
local function Tween(obj, props, time, style, dir)
    TweenService:Create(obj, TweenInfo.new(time or 0.3, style or Enum.EasingStyle.Quart, dir or Enum.EasingDirection.Out), props):Play()
end

local function Ripple(obj)
    spawn(function()
        local Mouse = Players.LocalPlayer:GetMouse()
        local Circle = Instance.new("ImageLabel")
        Circle.Name = "Ripple"
        Circle.Parent = obj
        Circle.BackgroundColor3 = Library.Theme.Text
        Circle.BackgroundTransparency = 1
        Circle.Image = "rbxassetid://266543268"
        Circle.ImageColor3 = Color3.new(1,1,1)
        Circle.ImageTransparency = 0.8
        
        local x, y = Mouse.X - obj.AbsolutePosition.X, Mouse.Y - obj.AbsolutePosition.Y
        Circle.Position = UDim2.new(0, x, 0, y)
        Circle.Size = UDim2.new(0,0,0,0)
        Circle.ZIndex = 10
        
        Tween(Circle, {Size = UDim2.new(0, 400, 0, 400), ImageTransparency = 1}, 0.5)
        wait(0.5)
        Circle:Destroy()
    end)
end

local function SaveConfig(platform)
    if writefile then
        pcall(function()
            local data = HttpService:JSONEncode({Mode = platform})
            writefile(ConfigName, data)
        end)
    end
end

local function LoadConfig()
    if isfile and isfile(ConfigName) then
        local success, result = pcall(function()
            return HttpService:JSONDecode(readfile(ConfigName))
        end)
        if success then return result.Mode end
    end
    return nil
end

function Library:ResetConfig()
    if delfile and isfile(ConfigName) then
        delfile(ConfigName)
    end
end

--// Main Window
function Library:Window(options)
    local Title = options.Title or "Velour v4"
    local Mode = LoadConfig()
    
    local ScreenGui = Instance.new("ScreenGui")
    ScreenGui.Name = "VelourUI"
    ScreenGui.Parent = RunService:IsStudio() and Players.LocalPlayer.PlayerGui or CoreGui
    ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

    --// Platform Picker (If no config)
    if not Mode then
        local Blur = Instance.new("Frame")
        Blur.Parent = ScreenGui
        Blur.BackgroundColor3 = Color3.new(0,0,0)
        Blur.BackgroundTransparency = 1
        Blur.Size = UDim2.new(1,0,1,0)
        Tween(Blur, {BackgroundTransparency = 0.4}, 0.5)

        local Picker = Instance.new("Frame")
        Picker.Parent = Blur
        Picker.BackgroundColor3 = Library.Theme.Main
        Picker.Size = UDim2.new(0, 0, 0, 0)
        Picker.Position = UDim2.new(0.5,0,0.5,0)
        Picker.AnchorPoint = Vector2.new(0.5,0.5)
        Picker.ClipsDescendants = true
        
        local PCorn = Instance.new("UICorner")
        PCorn.CornerRadius = UDim.new(0,10)
        PCorn.Parent = Picker
        
        Tween(Picker, {Size = UDim2.new(0, 300, 0, 180)}, 0.5, Enum.EasingStyle.Back)
        
        local Q = Instance.new("TextLabel")
        Q.Parent = Picker
        Q.Size = UDim2.new(1,0,0,40)
        Q.Position = UDim2.new(0,0,0,10)
        Q.BackgroundTransparency = 1
        Q.Text = "Select Device"
        Q.Font = Enum.Font.GothamBold
        Q.TextColor3 = Library.Theme.Text
        Q.TextSize = 18

        local function MakeBtn(text, pos)
            local Btn = Instance.new("TextButton")
            Btn.Parent = Picker
            Btn.Size = UDim2.new(0, 100, 0, 80)
            Btn.Position = pos
            Btn.BackgroundColor3 = Library.Theme.Section
            Btn.Text = text
            Btn.TextColor3 = Library.Theme.Text
            Btn.Font = Enum.Font.GothamBold
            Btn.TextSize = 16
            
            local C = Instance.new("UICorner"); C.Parent = Btn
            local S = Instance.new("UIStroke"); S.Parent = Btn; S.Color = Library.Theme.Outline; S.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            
            Btn.MouseEnter:Connect(function() Tween(S, {Color = Library.Theme.Accent}, 0.2) end)
            Btn.MouseLeave:Connect(function() Tween(S, {Color = Library.Theme.Outline}, 0.2) end)
            
            Btn.MouseButton1Click:Connect(function()
                Mode = text
                SaveConfig(text)
                Tween(Picker, {Size = UDim2.new(0,0,0,0)}, 0.3, Enum.EasingStyle.Back, Enum.EasingDirection.In)
                Tween(Blur, {BackgroundTransparency = 1}, 0.3)
                wait(0.3)
                Blur:Destroy()
            end)
        end
        
        MakeBtn("Desktop", UDim2.new(0, 30, 0, 60))
        MakeBtn("Mobile", UDim2.new(1, -130, 0, 60))
        
        repeat wait() until Mode ~= nil
    end

    local IsMobile = (Mode == "Mobile")
    local WinSize = IsMobile and UDim2.new(0, 500, 0, 300) or UDim2.new(0, 600, 0, 380)
    local ContentWidth = IsMobile and 120 or 150

    --// UI Construction
    local Main = Instance.new("Frame")
    Main.Name = "MainFrame"
    Main.Parent = ScreenGui
    Main.BackgroundColor3 = Library.Theme.Main
    Main.Size = UDim2.new(0,0,0,0) -- Anim Start
    Main.Position = UDim2.new(0.5,0,0.5,0)
    Main.AnchorPoint = Vector2.new(0.5,0.5)
    Main.ClipsDescendants = true
    
    local MainCorner = Instance.new("UICorner"); MainCorner.CornerRadius = UDim.new(0,8); MainCorner.Parent = Main
    local MainStroke = Instance.new("UIStroke"); MainStroke.Parent = Main; MainStroke.Color = Library.Theme.Outline; MainStroke.Thickness = 1.5

    -- Sidebar
    local Sidebar = Instance.new("Frame")
    Sidebar.Parent = Main
    Sidebar.BackgroundColor3 = Library.Theme.Sidebar
    Sidebar.Size = UDim2.new(0, ContentWidth, 1, 0)
    Sidebar.BorderSizePixel = 0
    
    local SideGradient = Instance.new("UIGradient")
    SideGradient.Rotation = 45
    SideGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0, Color3.fromRGB(255,255,255)), ColorSequenceKeypoint.new(1, Color3.fromRGB(200,200,200))}
    SideGradient.Parent = Sidebar

    local TitleLbl = Instance.new("TextLabel")
    TitleLbl.Parent = Sidebar
    TitleLbl.Text = Title
    TitleLbl.Font = Enum.Font.GothamBlack
    TitleLbl.TextColor3 = Library.Theme.Text
    TitleLbl.TextSize = 20
    TitleLbl.BackgroundTransparency = 1
    TitleLbl.Size = UDim2.new(1, -20, 0, 50)
    TitleLbl.Position = UDim2.new(0, 15, 0, 0)
    TitleLbl.TextXAlignment = Enum.TextXAlignment.Left

    local TabContainer = Instance.new("ScrollingFrame")
    TabContainer.Parent = Sidebar
    TabContainer.BackgroundTransparency = 1
    TabContainer.Size = UDim2.new(1,0,1,-60)
    TabContainer.Position = UDim2.new(0,0,0,60)
    TabContainer.ScrollBarThickness = 0
    
    local TabList = Instance.new("UIListLayout")
    TabList.Parent = TabContainer
    TabList.SortOrder = Enum.SortOrder.LayoutOrder
    TabList.Padding = UDim.new(0, 5)

    -- Content
    local Pages = Instance.new("Frame")
    Pages.Parent = Main
    Pages.BackgroundTransparency = 1
    Pages.Size = UDim2.new(1, -(ContentWidth + 20), 1, -20)
    Pages.Position = UDim2.new(0, ContentWidth + 10, 0, 10)
    Pages.ClipsDescendants = true

    -- Open Animation
    Tween(Main, {Size = WinSize}, 0.6, Enum.EasingStyle.Elastic)

    -- Dragging
    local Dragging, DragInput, DragStart, StartPos
    local function Update(input)
        local delta = input.Position - DragStart
        Main.Position = UDim2.new(StartPos.X.Scale, StartPos.X.Offset + delta.X, StartPos.Y.Scale, StartPos.Y.Offset + delta.Y)
    end
    Sidebar.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            Dragging = true; DragStart = input.Position; StartPos = Main.Position
            input.Changed:Connect(function() if input.UserInputState == Enum.UserInputState.End then Dragging = false end end)
        end
    end)
    Sidebar.InputChanged:Connect(function(input) if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then DragInput = input end end)
    UserInputService.InputChanged:Connect(function(input) if input == DragInput and Dragging then Update(input) end end)

    local Tabs = {}
    local FirstTab = true

    function Tabs:Tab(name, iconId)
        local TabBtn = Instance.new("TextButton")
        TabBtn.Parent = TabContainer
        TabBtn.BackgroundColor3 = Library.Theme.Main
        TabBtn.BackgroundTransparency = 1
        TabBtn.Size = UDim2.new(1, -10, 0, 36)
        TabBtn.Position = UDim2.new(0, 5, 0, 0)
        TabBtn.Text = "    " .. name
        TabBtn.TextColor3 = Library.Theme.TextDim
        TabBtn.Font = Enum.Font.GothamMedium
        TabBtn.TextSize = 14
        TabBtn.TextXAlignment = Enum.TextXAlignment.Left
        TabBtn.AutoButtonColor = false
        
        local TabCorner = Instance.new("UICorner"); TabCorner.CornerRadius = UDim.new(0,6); TabCorner.Parent = TabBtn
        local TabBar = Instance.new("Frame"); TabBar.Parent = TabBtn; TabBar.BackgroundColor3 = Library.Theme.Accent; TabBar.Size = UDim2.new(0, 3, 0, 16); TabBar.Position = UDim2.new(0,0,0.5,-8); TabBar.BackgroundTransparency = 1; local TBC = Instance.new("UICorner"); TBC.Parent = TabBar

        local Page = Instance.new("ScrollingFrame")
        Page.Parent = Pages
        Page.Size = UDim2.new(1,0,1,0)
        Page.BackgroundTransparency = 1
        Page.ScrollBarThickness = 2
        Page.ScrollBarImageColor3 = Library.Theme.Accent
        Page.Visible = false
        
        local PageList = Instance.new("UIListLayout"); PageList.Parent = Page; PageList.Padding = UDim.new(0, 6); PageList.SortOrder = Enum.SortOrder.LayoutOrder

        local function Activate()
            for _,v in pairs(Pages:GetChildren()) do if v:IsA("ScrollingFrame") then v.Visible = false end end
            for _,v in pairs(TabContainer:GetChildren()) do
                if v:IsA("TextButton") then
                    Tween(v, {BackgroundTransparency = 1, TextColor3 = Library.Theme.TextDim}, 0.3)
                    Tween(v.Frame, {BackgroundTransparency = 1}, 0.3)
                end
            end
            Page.Visible = true
            Tween(TabBtn, {BackgroundTransparency = 0, BackgroundColor3 = Color3.fromRGB(35,35,40), TextColor3 = Library.Theme.Text}, 0.3)
            Tween(TabBar, {BackgroundTransparency = 0}, 0.3)
        end
        
        TabBtn.MouseButton1Click:Connect(Activate)
        if FirstTab then FirstTab = false; Activate() end

        local Elements = {}
        
        function Elements:Section(text)
            local Sec = Instance.new("TextLabel")
            Sec.Parent = Page
            Sec.Size = UDim2.new(1,0,0,30)
            Sec.BackgroundTransparency = 1
            Sec.Text = text
            Sec.Font = Enum.Font.GothamBold
            Sec.TextColor3 = Library.Theme.Accent
            Sec.TextSize = 12
            Sec.TextXAlignment = Enum.TextXAlignment.Left
            Sec.TextTransparency = 0.5
            Instance.new("UIPadding", Sec).PaddingLeft = UDim.new(0, 2)
        end

        function Elements:Button(text, callback)
            local Btn = Instance.new("TextButton")
            Btn.Parent = Page
            Btn.BackgroundColor3 = Library.Theme.Section
            Btn.Size = UDim2.new(1, -4, 0, 38)
            Btn.AutoButtonColor = false
            Btn.Text = ""
            
            local UIC = Instance.new("UICorner"); UIC.Parent = Btn; UIC.CornerRadius = UDim.new(0,6)
            local UIS = Instance.new("UIStroke"); UIS.Parent = Btn; UIS.Color = Library.Theme.Outline; UIS.ApplyStrokeMode = Enum.ApplyStrokeMode.Border

            local Lbl = Instance.new("TextLabel")
            Lbl.Parent = Btn
            Lbl.BackgroundTransparency = 1
            Lbl.Size = UDim2.new(1, -10, 1, 0)
            Lbl.Position = UDim2.new(0, 10, 0, 0)
            Lbl.Text = text
            Lbl.Font = Enum.Font.Gotham
            Lbl.TextColor3 = Library.Theme.Text
            Lbl.TextSize = 14
            Lbl.TextXAlignment = Enum.TextXAlignment.Left
            
            local Icon = Instance.new("ImageLabel"); Icon.Parent = Btn; Icon.BackgroundTransparency = 1; Icon.Image = "rbxassetid://6031063555"; Icon.Size = UDim2.new(0,20,0,20); Icon.Position = UDim2.new(1,-30,0.5,-10); Icon.ImageColor3 = Library.Theme.TextDim

            Btn.MouseEnter:Connect(function() Tween(UIS, {Color = Library.Theme.Accent}, 0.2) end)
            Btn.MouseLeave:Connect(function() Tween(UIS, {Color = Library.Theme.Outline}, 0.2) end)
            Btn.MouseButton1Click:Connect(function() Ripple(Btn); callback() end)
        end

        function Elements:Toggle(text, default, callback)
            local toggled = default or false
            local Tog = Instance.new("TextButton")
            Tog.Parent = Page
            Tog.BackgroundColor3 = Library.Theme.Section
            Tog.Size = UDim2.new(1, -4, 0, 38)
            Tog.AutoButtonColor = false; Tog.Text = ""
            
            local UIC = Instance.new("UICorner"); UIC.Parent = Tog; UIC.CornerRadius = UDim.new(0,6)
            local UIS = Instance.new("UIStroke"); UIS.Parent = Tog; UIS.Color = Library.Theme.Outline
            
            local Lbl = Instance.new("TextLabel"); Lbl.Parent = Tog; Lbl.BackgroundTransparency = 1; Lbl.Text = text; Lbl.Font = Enum.Font.Gotham; Lbl.TextColor3 = Library.Theme.Text; Lbl.TextSize = 14; Lbl.Position = UDim2.new(0,10,0,0); Lbl.Size = UDim2.new(1,-60,1,0); Lbl.TextXAlignment = Enum.TextXAlignment.Left

            local Switch = Instance.new("Frame"); Switch.Parent = Tog; Switch.BackgroundColor3 = Library.Theme.Main; Switch.Size = UDim2.new(0,40,0,20); Switch.Position = UDim2.new(1,-50,0.5,-10); local SC = Instance.new("UICorner"); SC.CornerRadius = UDim.new(1,0); SC.Parent = Switch
            local Dot = Instance.new("Frame"); Dot.Parent = Switch; Dot.BackgroundColor3 = Library.Theme.TextDim; Dot.Size = UDim2.new(0,16,0,16); Dot.Position = UDim2.new(0,2,0.5,-8); local DC = Instance.new("UICorner"); DC.CornerRadius = UDim.new(1,0); DC.Parent = Dot

            local function Update()
                local tP = toggled and UDim2.new(1,-18,0.5,-8) or UDim2.new(0,2,0.5,-8)
                local tC = toggled and Library.Theme.Accent or Library.Theme.TextDim
                Tween(Dot, {Position = tP, BackgroundColor3 = tC}, 0.2)
                callback(toggled)
            end
            
            if default then Update() end
            
            Tog.MouseButton1Click:Connect(function() toggled = not toggled; Update() end)
        end

        function Elements:Slider(text, min, max, default, callback)
            local value = default or min
            local Sli = Instance.new("Frame")
            Sli.Parent = Page
            Sli.BackgroundColor3 = Library.Theme.Section
            Sli.Size = UDim2.new(1, -4, 0, 50)
            local UIC = Instance.new("UICorner"); UIC.Parent = Sli; UIC.CornerRadius = UDim.new(0,6)
            local UIS = Instance.new("UIStroke"); UIS.Parent = Sli; UIS.Color = Library.Theme.Outline

            local Lbl = Instance.new("TextLabel"); Lbl.Parent = Sli; Lbl.Text = text; Lbl.Font = Enum.Font.Gotham; Lbl.TextColor3 = Library.Theme.Text; Lbl.TextSize = 14; Lbl.Position = UDim2.new(0,10,0,5); Lbl.Size = UDim2.new(1,-20,0,20); Lbl.BackgroundTransparency = 1; Lbl.TextXAlignment = Enum.TextXAlignment.Left
            local Val = Instance.new("TextLabel"); Val.Parent = Sli; Val.Text = tostring(value); Val.Font = Enum.Font.Gotham; Val.TextColor3 = Library.Theme.TextDim; Val.TextSize = 14; Val.Position = UDim2.new(1,-50,0,5); Val.Size = UDim2.new(0,40,0,20); Val.BackgroundTransparency = 1

            local Bar = Instance.new("TextButton"); Bar.Parent = Sli; Bar.BackgroundColor3 = Library.Theme.Main; Bar.Size = UDim2.new(1,-20,0,6); Bar.Position = UDim2.new(0,10,0,32); Bar.Text = ""; Bar.AutoButtonColor = false; local BC = Instance.new("UICorner"); BC.Parent = Bar
            local Fill = Instance.new("Frame"); Fill.Parent = Bar; Fill.BackgroundColor3 = Library.Theme.Accent; Fill.Size = UDim2.new((value-min)/(max-min),0,1,0); local FC = Instance.new("UICorner"); FC.Parent = Fill

            local Dragging = false
            local function Update(input)
                local sX = math.clamp((input.Position.X - Bar.AbsolutePosition.X) / Bar.AbsoluteSize.X, 0, 1)
                local newVal = math.floor(min + ((max - min) * sX))
                Tween(Fill, {Size = UDim2.new(sX,0,1,0)}, 0.05)
                Val.Text = tostring(newVal)
                callback(newVal)
            end
            
            Bar.InputBegan:Connect(function(i) if i.UserInputType == Enum.UserInputType.MouseButton1 or i.UserInputType == Enum.UserInputType.Touch then Dragging = true; Update(i) end end)
            UserInputService.InputEnded:Connect(function(i) if i.UserInputType == Enum.UserInputType.MouseButton1 or i.UserInputType == Enum.UserInputType.Touch then Dragging = false end end)
            UserInputService.InputChanged:Connect(function(i) if Dragging and (i.UserInputType == Enum.UserInputType.MouseMovement or i.UserInputType == Enum.UserInputType.Touch) then Update(i) end end)
        end
        
        function Elements:Dropdown(text, options, callback)
            local Dropped = false
            local Drop = Instance.new("Frame")
            Drop.Parent = Page
            Drop.BackgroundColor3 = Library.Theme.Section
            Drop.Size = UDim2.new(1, -4, 0, 38)
            Drop.ClipsDescendants = true
            
            local UIC = Instance.new("UICorner"); UIC.Parent = Drop; UIC.CornerRadius = UDim.new(0,6)
            local UIS = Instance.new("UIStroke"); UIS.Parent = Drop; UIS.Color = Library.Theme.Outline

            local Btn = Instance.new("TextButton"); Btn.Parent = Drop; Btn.Size = UDim2.new(1,0,0,38); Btn.BackgroundTransparency = 1; Btn.Text = ""; 
            local Lbl = Instance.new("TextLabel"); Lbl.Parent = Btn; Lbl.Text = text .. " ..."; Lbl.Font = Enum.Font.Gotham; Lbl.TextColor3 = Library.Theme.Text; Lbl.TextSize = 14; Lbl.Position = UDim2.new(0,10,0,0); Lbl.Size = UDim2.new(1,-40,1,0); Lbl.BackgroundTransparency = 1; Lbl.TextXAlignment = Enum.TextXAlignment.Left
            local Icon = Instance.new("ImageLabel"); Icon.Parent = Btn; Icon.Image = "rbxassetid://6031090990"; Icon.Size = UDim2.new(0,20,0,20); Icon.Position = UDim2.new(1,-30,0.5,-10); Icon.BackgroundTransparency = 1; Icon.ImageColor3 = Library.Theme.TextDim

            local List = Instance.new("UIListLayout"); List.Parent = Drop; List.SortOrder = Enum.SortOrder.LayoutOrder; List.Padding = UDim.new(0,2)
            Instance.new("UIPadding", Drop).PaddingTop = UDim.new(0, 38)

            for _, opt in pairs(options) do
                local OptBtn = Instance.new("TextButton")
                OptBtn.Parent = Drop
                OptBtn.Size = UDim2.new(1,0,0,30)
                OptBtn.BackgroundColor3 = Library.Theme.Main
                OptBtn.BackgroundTransparency = 0.5
                OptBtn.Text = opt
                OptBtn.TextColor3 = Library.Theme.TextDim
                OptBtn.Font = Enum.Font.Gotham
                OptBtn.TextSize = 13
                
                OptBtn.MouseButton1Click:Connect(function()
                    Lbl.Text = text .. ": " .. opt
                    callback(opt)
                    Dropped = false
                    Tween(Drop, {Size = UDim2.new(1, -4, 0, 38)}, 0.3)
                    Tween(Icon, {Rotation = 0}, 0.3)
                end)
            end

            Btn.MouseButton1Click:Connect(function()
                Dropped = not Dropped
                local h = Dropped and (38 + (#options * 32) + 5) or 38
                Tween(Drop, {Size = UDim2.new(1, -4, 0, h)}, 0.3, Enum.EasingStyle.Back)
                Tween(Icon, {Rotation = Dropped and 180 or 0}, 0.3)
            end)
        end

        return Elements
    end
    
    function Library:Notify(text, duration)
        local Notif = Instance.new("Frame")
        Notif.Parent = ScreenGui
        Notif.BackgroundColor3 = Library.Theme.Main
        Notif.Size = UDim2.new(0, 250, 0, 50)
        Notif.Position = UDim2.new(1, 20, 0.8, 0) -- Off screen
        
        local NC = Instance.new("UICorner"); NC.Parent = Notif
        local NS = Instance.new("UIStroke"); NS.Parent = Notif; NS.Color = Library.Theme.Accent
        
        local Lbl = Instance.new("TextLabel"); Lbl.Parent = Notif; Lbl.Size = UDim2.new(1,-20,1,0); Lbl.Position = UDim2.new(0,10,0,0); Lbl.BackgroundTransparency = 1; Lbl.Text = text; Lbl.TextColor3 = Library.Theme.Text; Lbl.Font = Enum.Font.GothamBold; Lbl.TextSize = 14; Lbl.TextWrapped = true

        Tween(Notif, {Position = UDim2.new(1, -270, 0.8, 0)}, 0.5, Enum.EasingStyle.Back)
        delay(duration or 3, function()
            Tween(Notif, {Position = UDim2.new(1, 20, 0.8, 0)}, 0.5, Enum.EasingStyle.Back, Enum.EasingDirection.In)
            wait(0.5); Notif:Destroy()
        end)
    end

    -- Toggle Keybind
    UserInputService.InputBegan:Connect(function(input, gp)
        if gp then return end
        if input.KeyCode == Enum.KeyCode.RightControl then
            ScreenGui.Enabled = not ScreenGui.Enabled
        end
    end)
    
    return Tabs
end

return Library
